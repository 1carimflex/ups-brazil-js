{
  "tags": {
    "allowUnknownTags": false
  },
  "source": {
    "include": "src",
    "includePattern": "\\.js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "opts": {
    "readme": "README.md",
    "template": "node_modules/better-docs",
    "encoding": "utf8",
    "destination": "docs/",
    "recurse": true,
    "verbose": true
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "better-docs": {
      "name": "UPS Brazil JS - By CarimFlex",
      "hideGenerator": false,
      "navigation": [
        {
          "label": "Github",
          "href": "https://github.com/1carimflex/ups-brazil-js"
        }
      ]
    }
  }
}
